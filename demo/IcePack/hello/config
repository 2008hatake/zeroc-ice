IcePack.Registry.Data=db/registry

#
# IcePack node configuration.
#
IcePack.Node.Name=node
IcePack.Node.Endpoints=default
IcePack.Node.Data=db/node
IcePack.Node.CollocateRegistry=1

#
# Trace properties.
